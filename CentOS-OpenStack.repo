enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Cloud
exclude=sip,PyQt4
module_hotfixes=1

[centos-openstack-OPENSTACK_VERSION-source]
