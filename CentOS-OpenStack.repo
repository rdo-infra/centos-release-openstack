# CentOS-OpenStack-OPENSTACK_VERSION.repo
#
# Please see http://wiki.centos.org/SpecialInterestGroup/Cloud for more
# information

[centos-rdo-openstack-OPENSTACK_VERSION]
name=CentOS-$releasever - RDO OpenStack OPENSTACK_VERSION
baseurl=http://mirror.centos.org/centos/$releasever/cloud/$basearch/openstack-OPENSTACK_VERSION/
gpgcheck=1
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Cloud

[centos-rdo-openstack-OPENSTACK_VERSION-test]
name=CentOS-$releasever - RDO OpenStack OPENSTACK_VERSION Testing
baseurl=http://buildlogs.centos.org/centos/$releasever/cloud/$basearch/openstack-OPENSTACK_VERSION/
gpgcheck=0
enabled=1
